Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.autohint:   false
Xft.lcdfilter:  lcddefault

Emacs*toolBar: off
Emacs*menuBar: off
Emacs.verticalScrollBars: off

! special
URxvt*.foreground:   #f1ebeb
URxvt*.background:   #272822
URxvt*.cursorColor:  #f1ebeb

! black
URxvt*.color0:       #48483e
URxvt*.color8:       #76715e

! red
URxvt*.color1:       #dc2566
URxvt*.color9:       #fa2772

! green
URxvt*.color2:       #8fc029
URxvt*.color10:      #a7e22e

! yellow
URxvt*.color3:       #d4c96e
URxvt*.color11:      #e7db75

! blue
URxvt*.color4:       #55bcce
URxvt*.color12:      #66d9ee

! magenta
URxvt*.color5:       #9358fe
URxvt*.color13:      #ae82ff

! cyan
URxvt*.color6:       #56b7a5
URxvt*.color14:      #66efd5

! white
URxvt*.color7:       #acada1
URxvt*.color15:      #cfd0c2

URxvt.scrollBar: False
URxvt.font:xft:InconsolataGo Nerd Font Mono:Regular:pixelsize=22,xft:ZhunYuan:Regular:pixelsize=20,xft:Symbola:Regular:pixelsize=14
URxvt.boldfont:xft:InconsolataGo Nerd Font Mono:Bold:pixelsize=22,xft:ZhunYuan:Bold:pixelsize=20,xft:Symbola:Bold:pixelsize=14


!-*- Perl extensions -*-
URxvt.perl-ext-common: default,selection-to-clipboard,pasta,matcher,keyboard-select
URxvt.keysym.M-u:     perl:url-select:select_next
URxvt.url-launcher:   /usr/bin/google-chrome-stable
URxvt.underlineURLs:  True
URxvt.matcher.button: 1
URxvt.keysym.M-Escape:perl:keyboard-select:activate
URxvt.keysym.Control-Shift-V:     perl:pasta:paste
URxvt.clipboard.autocopy: true
